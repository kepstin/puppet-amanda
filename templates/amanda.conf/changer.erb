define changer "<%= @changer_name %>" {
<% @inherit.each do |name| -%>
  <%= name %>
<% end -%>
  tpchanger "<%= @tpchanger %>"
<% if @changerdev -%>
  changerdev "<%= @changerdev %>"
<% end -%>
<% if @changerfile -%>
  changerfile "<%= @changerfile %>"
<% end -%>
<% if @tapedev -%>
  tapedev "<%= @tapedev %>"
<% end -%>
<% @property.keys.sort.each do |k| -%>
  property "<%= k %>"<% if @property[v].respond_to?('each') %><% @property[k].each do |v| %> "<%= v %>"<% end %><% else %> "<%= @property[k] %>"<% end %>
<%- end %>
}
