define changer "<%= @changer_name %>" {
<% @inherit.each do |name| -%>
  <%= name %>
<% end -%>
  tpchanger "<%= @tpchanger %>"
<% if @changerdev -%>
  changerdev "<%= @changerdev %>"
<% end -%>
<% if @changerfile -%>
  changerfile "<%= @changerfile %>"
<% end -%>
<% if @tapedev -%>
  tapedev "<%= @tapedev %>"
<% end -%>
<% @property.each_pair do |k, v| -%>
  property "<%= k %>"<%- if v.respond_to?('each') -%>
    <%- v.each do |vi| %> "<%= vi %>"<%- end -%>
  <%- else -%>
    "<%= v %>"<%- end %>
<%- end %>
}
