<% end -%>
infofile "<%= @infofile %>"
logdir "<%= @logdir %>"
indexdir "<%= @indexdir %>"
dumpuser "<%= @user %>"

<% if @dumpcycle -%>
dumpcycle <%= @dumpcycle %>
<% end -%>
<% if @runspercycle -%>
runspercycle <%= @runspercycle %>
<% end -%>
<% if @tapecycle -%>
tapecycle <%= @tapecycle %>
<% end -%>
<% if @runtapes -%>
runtapes <%= @runtapes %>
<% end -%>

<% if @tapetype -%>
tapetype "<%= @tapetype %>"
<% end -%>
<% if @tpchanger -%>
tpchanger "<%= @tpchanger %>"
<% end -%>

