# This configuration file is managed by Puppet. Please do not edit it manually.

<% if @org -%>
org "<%= @org %>"
<% end -%>
infofile "<%= @infofile %>"
logdir "<%= @logdir %>"
indexdir "<%= @indexdir %>"
dumpuser "<%= @user %>"

<% if @dumpcycle -%>
dumpcycle <%= @dumpcycle %>
<% end -%>
<% if @runspercycle -%>
runspercycle <%= @runspercycle %>
<% end -%>
<% if @tapecycle -%>
dumpcycle <%= @tapecycle %>
<% end -%>
<% if @tpchanger -%>
tpchanger "<%= @tpchanger %>"
<% end -%>
<% @extra_config.each_pair do |k, v| -%>
<%= k %> <%= v %>
<% end -%>

